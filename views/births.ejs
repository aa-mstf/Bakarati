<%- include('partials/header'); -%>
<h2 id="t-title">Birth List &#128046;</h2>
<div class="crud">
  <a class="btn Add" href="/AddBirth">Add</a>
  <a class="btn delete" href="/DeleteBirth">Delete</a>
  <a class="btn Modify" href="/ModifyBirth">Modify</a>
</div>
<table id="customers">
    <thead>
    <tr>
      <th>ID</th>
      <th>Mother ID</th>
      <th>Date</th>
    </tr>
</thead>
<tbody>
    <% for (var i = 0; i < Items.length ; i++) { %>
        <tr>
            <td><%= Items[i].id %></td>
             <td><%= Items[i].motherId %></td>
            <td> <%= Items[i].date %></td>
        </tr>
    <% } %>
</tbody>
  </table>
<%- include('partials/footer'); -%>