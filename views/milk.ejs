<%- include('partials/header'); -%>
<h2 id="t-title">Milk Amount List &#129371;</h2>
<div class="crud">
  <a class="btn Add" href="/AddMilk">Add</a>
  <a class="btn delete" href="/DeleteMilk">Delete</a>
  <a class="btn Modify" href="/ModifyMilk">Modify</a>
</div>
<table id="customers">
    <thead>
    <tr>
      <th>No.</th>
      <th>Amount</th>
      <th>Date</th>
    </tr>
</thead>
<tbody>
    <% for (var i = 0; i < Items.length ; i++) { %>
        <tr>
             <td><%= Items[i].id %></td>
            <td><%= Items[i].Amount %></td>
            <td> <%= Items[i].date %></td>
        </tr>
    <% } %>
</tbody>
  </table>
<%- include('partials/footer'); -%>